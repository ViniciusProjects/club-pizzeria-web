<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Pizzas</servlet-name>
		<display-name>Pizzas</display-name>
		<description></description>
		<servlet-class>br.com.clubpizzeria.Pizzas</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FuncionarioServlet</servlet-name>
		<display-name>FuncionarioServlet</display-name>
		<description></description>
		<servlet-class>br.com.clubpizzeria.FuncionarioServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Pizzas</servlet-name>
		<url-pattern>/Pizzas</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FuncionarioServlet</servlet-name>
		<url-pattern>/FuncionarioServlet</url-pattern>
	</servlet-mapping>
</web-app>